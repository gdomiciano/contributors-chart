{
  "name": "contributors-chart",
  "version": "1.0.0",
  "description": "Responsive application, that user can search for a repository and see contributions per user in a chart.",
  "author": "Geisy Domiciano <geisydomiciano@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "build:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/proxy": "^1.3.1",
    "@nuxtjs/pwa": "^2.6.0",
    "acorn": "^6.0.4",
    "acorn-jsx": "^5.0.0",
    "nuxt": "latest",
    "vue-chartjs": "^3.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chart.js": "^2.7.3",
    "cross-env": "^5.2.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-vue": "^4.7.1",
    "node-sass": "^4.10.0",
    "push-dir": "^0.4.1",
    "sass-loader": "^7.1.0"
  }
}
